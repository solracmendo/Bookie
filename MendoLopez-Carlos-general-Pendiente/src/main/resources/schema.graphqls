type Tarea {
	id: ID!
	nombre: String!
	identificador_Personal: String!
	servicio: String!
	email: String!
}

type Query {
	allTareas: [Tarea]
	misTareas(email: String!): [Tarea]
}

type Mutation {
	createTarea(nombre: String!, servicio: String!, identificador: String!, email: String!): Tarea
	completarTarea(correo: String!, identificador: String!, servicio: String!) : Boolean
}

schema {
  query: Query
  mutation: Mutation
}